pipeline {
    
    agent any

    environment {
        CI = 'true'
    }

    stages {
        stage('Install') { 
            steps {
                bat 'npm install --registry=https://registry.npm.taobao.org'
            }
        }

        stage('Deploy') { 
            steps {
                bat 'npm run-script build' 
                bat 'xcopy build\\* D:\\deploy\\frontend /e /y'
            }
        }
    }
}